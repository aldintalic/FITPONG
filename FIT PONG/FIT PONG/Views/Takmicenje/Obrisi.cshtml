
@{
    ViewData["Title"] = "Obrisi";
    Takmicenje x = ViewBag.takmicenje;
}

<form action="/Takmicenje/PotvrdaBrisanja" method="post">
    <h1>Da li ste sigurni da zelite obrisati :</h1>
    <h2>@x.Naziv takmicenje</h2>
    @Html.Hidden("ID", x.ID)
    <input type="button" value="NE" onclick="window.location.href='/Takmicenje'"/>
    <input type="submit" value="DA" />
</form>
